{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"target": "es6",
		"module": "commonjs",
		"allowJs": true,
		"checkJs": false,
		"baseUrl": "./src",
		"typeRoots": ["./typings/", "./node_modules/@types/"],
		"paths": {
			"@assets/*": ["../assets/*"],
			"@components/*": ["./components/*"],
			"@contexts/*": ["./contexts/*"],
			"@gql/*": ["./gql/*"],
			"@pages/*": ["./pages/*"],
			"@routes/*": ["./contexts/*"],
			"@services/*": ["./services/*"],
			"@typings/*": ["./typings/*"]
		}
	}
}
